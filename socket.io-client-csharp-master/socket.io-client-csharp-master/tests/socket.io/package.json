{
  "name": "socket.io",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "v4-ws": "cd v4 && npm run v4-ws",
    "v4-ws-token": "cd v4 && npm run v4-ws-token",
    "v4-http": "cd v4 && npm run v4-http",
    "v4-http-token": "cd v4 && npm run v4-http-token",

    "v4-ws-mp": "cd v4 && npm run v4-ws-mp",
    "v4-ws-token-mp": "cd v4 && npm run v4-ws-token-mp",
    "v4-http-mp": "cd v4 && npm run v4-http-mp",
    "v4-http-token-mp": "cd v4 && npm run v4-http-token-mp",
  
    "v2-ws": "cd v2 && npm run v2-ws",
    "v2-ws-token": "cd v2 && npm run v2-ws-token",
    "v2-http": "cd v2 && npm run v2-http",
    "v2-http-token": "cd v2 && npm run v2-http-token",

    "v2-ws-mp": "cd v2 && npm run v2-ws-mp",
    "v2-ws-token-mp": "cd v2 && npm run v2-ws-token-mp",
    "v2-http-mp": "cd v2 && npm run v2-http-mp",
    "v2-http-token-mp": "cd v2 && npm run v2-http-token-mp",

    "install":"cd v4 && npm install && cd ../v2 && npm install",
    "start": "concurrently \"npm run v4-ws\" \"npm run v4-ws-token\" \"npm run v4-http\" \"npm run v4-http-token\" \"npm run v4-ws-mp\" \"npm run v4-ws-token-mp\" \"npm run v4-http-mp\" \"npm run v4-http-token-mp\" \"npm run v2-ws\" \"npm run v2-ws-token\" \"npm run v2-http\" \"npm run v2-http-token\" \"npm run v2-ws-mp\" \"npm run v2-ws-token-mp\" \"npm run v2-http-mp\" \"npm run v2-http-token-mp\""
